  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "dev-host": "nuxt --port 8000",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",

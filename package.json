    "@smartweb/vue-flash-message": "^0.6.10",
    "@vue-a11y/dark-mode": "^1.1.1",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.21.2",
    "core-js": "^3.9.1",
    "jquery": "^3.6.0",
    "moment": "^2.29.1",
    "nuxt": "^2.15.3",
    "nuxt-sweetalert2": "^1.0.0",
